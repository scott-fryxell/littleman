svg
  padding:0
  margin:0

svg#logo
  fluid-size width, 3rem, 3.25rem
  fluid-size height, 1rem, 2.5rem
  stroke:blue
  fill:green-fill
  opacity:0.4
  position: fixed
  stroke-width:5

svg#hero
  z-index: -1
  width:100vw
  min-height: 100vh
  position:absolute
  top:0
  left:0
  opacity:1
  fill:green-fill
  stroke:blue-fill
  stroke-width:0.5

  & use:nth-child(2)
    stroke:red
    stroke-width:0.1
    fill:transparent
